{
  "rules":{
    "baseRoute":"/collapsar",//表示该路由的基本前缀，下面的路由可用./来表示
    "ruleState":"on"//表示是否要使用下述路由规则，on表示使用，off表示不使用，使用默认路径进行寻找
  },
  "staticRouteRules": [
    {
      "staticRoutes":[
        "./index.html"
      ],
      "filepath":"index.html"//静态文件位置在jar包内的Resources/位置内,文件自动加上了Resources/前缀
    }
  ],
  "dynamicActions":[
    {
      "dynamicRoutes":[
        "./collapsarUser"
      ],
      "className":"nju.edu.collapsarUser.CollapsarUser"
    }
  ]
}